{
  "name": "abyssal-discord-bot",
  "version": "1.0.0",
  "description": "discord bot that manages guild activities for Abyssal and Azurlane",
  "main": "index.js",
  "scripts": {
    "start": "ts-node src/index.ts",
    "dev:watch": "ts-node-dev src/index.ts",
    "prod": "ts-node --transpile-only src/index.ts",
    "lint": "eslint --ignore-path .eslintignore --ext .js,.ts .",
    "format": "prettier --ignore-path .gitignore --write \"**/*.+(js|ts|json)\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/peterbuoy/abyssal-discord-bot.git"
  },
  "author": "Peter Bui",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/peterbuoy/abyssal-discord-bot/issues"
  },
  "homepage": "https://github.com/peterbuoy/abyssal-discord-bot#readme",
  "dependencies": {
    "@discordjs/builders": "^0.11.0",
    "@types/better-sqlite3": "^7.4.2",
    "better-sqlite3": "^7.5.0",
    "discord.js": "^13.6.0",
    "dotenv": "^14.2.0"
  },
  "devDependencies": {
    "@types/node": "^17.0.10",
    "@typescript-eslint/eslint-plugin": "^5.10.0",
    "@typescript-eslint/parser": "^5.10.0",
    "eslint": "^8.7.0",
    "eslint-config-prettier": "^8.3.0",
    "prettier": "^2.5.1",
    "ts-node": "^10.4.0",
    "ts-node-dev": "^1.1.8",
    "typescript": "^4.5.4"
  }
}
